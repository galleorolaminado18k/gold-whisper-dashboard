# Local Development Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Chatwoot Configuration
VITE_CHATWOOT_URL=http://localhost:3020
VITE_CHATWOOT_ACCOUNT_ID=1
VITE_CHATWOOT_WEBSITE_TOKEN=your_website_token_here
VITE_CHATWOOT_API_TOKEN=your_api_token_here
VITE_CHATWOOT_ENABLED=true
VITE_FRONTEND_URL=http://localhost:8081
VITE_BRIDGE_API_URL=http://localhost:4000

# Meta Ads Configuration
VITE_META_BUSINESS_ID=your_business_id_here
VITE_META_ACCESS_TOKEN=your_meta_access_token_here
VITE_META_AD_ACCOUNT_IDS=account_id_1,account_id_2
VITE_META_AD_ACCOUNT_NAMES=Account Name 1,Account Name 2
VITE_META_AD_ACCOUNT_ID=your_primary_account_id_here
VITE_META_AD_ACCOUNT_NAME=your_primary_account_name_here

# Claude AI Configuration (Anthropic)
# Get your API key from: https://console.anthropic.com/settings/keys
VITE_CLAUDE_API_KEY=your_claude_api_key_here
VITE_CLAUDE_MODEL=claude-3-5-sonnet-20241022
VITE_CLAUDE_ENABLED=false

# Default AOV (Average Order Value) Settings
VITE_DEFAULT_AOV=285000
VITE_AOV_BALINERIA=285000
VITE_AOV_JOYERIA=285000

# Fallback Auth Configuration
VITE_FALLBACK_AUTH_EMAIL=demo@dashboard.galle18k.com
VITE_FALLBACK_AUTH_PASSWORD=demopass123
VITE_FALLBACK_AUTH_NAME=Usuario Demo
VITE_ENABLE_FALLBACK_AUTH=true
VITE_FALLBACK_ALLOWED_EMAILS=demo@dashboard.galle18k.com
